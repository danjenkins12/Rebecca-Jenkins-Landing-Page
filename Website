/* ═══ Market Update Data ═══ */
const MARKET_DATA = {
  updated: "January 2026",
  stats: [
    { label: "Median Sale Price", value: "$257,500", change: "+12.4% YoY", direction: "up" },
    { label: "Median Days on Market", value: "7 Days", change: "Fastest in U.S.", direction: "down" },
    { label: "Active Listings", value: "483", change: "Low inventory", direction: "down" },
    { label: "Homes Sold (Jan)", value: "70", change: "High demand", direction: "up" },
  ],
};

/* ═══ Main Component ═══ */
export default function RebeccaJenkinsLanding() {
  const [scrolled, setScrolled] = useState(false);
  const [activeTab, setActiveTab] = useState(0);
  const [tabProgress, setTabProgress] = useState(0);
  const tabTimerRef = useRef(null);
  const TAB_INTERVAL = 6000;

  const [form, setForm] = useState({ name: "", email: "", phone: "" });
  const [submitting, setSubmitting] = useState(false);
  const [submitted, setSubmitted] = useState(false);
  const [formError, setFormError] = useState("");

  const [valForm, setValForm] = useState({ address: "", name: "", email: "", phone: "" });
  const [valSubmitting, setValSubmitting] = useState(false);
  const [valSubmitted, setValSubmitted] = useState(false);
  const [valError, setValError] = useState("");

  const [marketOpen, setMarketOpen] = useState(false);

  useEffect(() => {
    const fn = () => setScrolled(window.scrollY > 40);
    window.addEventListener("scroll", fn, { passive: true });
    return () => window.removeEventListener("scroll", fn);
  }, []);

  /* Tab auto-cycle */
  const startTabTimer = useCallback(() => {
    if (tabTimerRef.current) clearInterval(tabTimerRef.current);
    setTabProgress(0);
    const startTime = Date.now();
    tabTimerRef.current = setInterval(() => {
      const elapsed = Date.now() - startTime;
      const pct = Math.min((elapsed / TAB_INTERVAL) * 100, 100);
      setTabProgress(pct);
      if (elapsed >= TAB_INTERVAL) {
        setActiveTab((p) => (p + 1) % TABS_DATA.length);
      }
    }, 50);
  }, []);

  useEffect(() => {
    startTabTimer();
    return () => clearInterval(tabTimerRef.current);
  }, [activeTab, startTabTimer]);

  const handleTabClick = (i) => {
    setActiveTab(i);
    clearInterval(tabTimerRef.current);
    startTabTimer();
  };

  /* Form submit */
  const handleSubmit = async (e) => {
    e.preventDefault();
    setFormError("");
    if (!form.name || !form.email || !form.phone) {
      setFormError("Please fill in all fields.");
      return;
    }
    setSubmitting(true);
    try {
      const res = await fetch(`https://formspree.io/f/${FORMSPREE_ID}`, {
        method: "POST",
        headers: { "Content-Type": "application/json", Accept: "application/json" },
        body: JSON.stringify(form),
      });
      /* Backup: Google Forms → Google Sheets (fire and forget) */
      if (GOOGLE_FORM_ID && !GOOGLE_FORM_ID.startsWith("YOUR")) {
        const gUrl = `https://docs.google.com/forms/d/e/${GOOGLE_FORM_ID}/formResponse`;
        const gData = new URLSearchParams({
          [GOOGLE_ENTRY_NAME]: form.name,
          [GOOGLE_ENTRY_EMAIL]: form.email,
          [GOOGLE_ENTRY_PHONE]: form.phone,
        });
        fetch(gUrl, { method: "POST", mode: "no-cors", body: gData }).catch(() => {});
      }
      if (res.ok) { setSubmitted(true); }
      else { setFormError("Something went wrong. Please try again."); }
    } catch {
      setFormError("Network error. Please try again or call directly.");
    }
    setSubmitting(false);
  };

  const isFormValid = form.name && form.email && form.phone;

  /* Valuation form submit */
  const handleValSubmit = async (e) => {
    e.preventDefault();
    setValError("");
    if (!valForm.address || !valForm.name || !valForm.email || !valForm.phone) {
      setValError("Please fill in all fields.");
      return;
    }
    setValSubmitting(true);
    try {
      const res = await fetch(`https://formspree.io/f/${FORMSPREE_ID}`, {
        method: "POST",
        headers: { "Content-Type": "application/json", Accept: "application/json" },
        body: JSON.stringify({ ...valForm, type: "Home Valuation Request" }),
      });
      if (GOOGLE_FORM_ID && !GOOGLE_FORM_ID.startsWith("YOUR")) {
        const gUrl = `https://docs.google.com/forms/d/e/${GOOGLE_FORM_ID}/formResponse`;
        const gData = new URLSearchParams({
          [GOOGLE_ENTRY_NAME]: valForm.name + " — HOME VALUATION: " + valForm.address,
          [GOOGLE_ENTRY_EMAIL]: valForm.email,
          [GOOGLE_ENTRY_PHONE]: valForm.phone,
        });
        fetch(gUrl, { method: "POST", mode: "no-cors", body: gData }).catch(() => {});
      }
      if (res.ok) { setValSubmitted(true); }
      else { setValError("Something went wrong. Please try again."); }
    } catch {
      setValError("Network error. Please try again or call directly.");
    }
    setValSubmitting(false);
  };

  const isValFormValid = valForm.address && valForm.name && valForm.email && valForm.phone;
  const tab = TABS_DATA[activeTab];

  return (
    <>
      <style>{CSS}</style>

      {/* ── Nav ── */}
      <nav className={`nav ${scrolled ? "scrolled" : ""}`}>
        <a href="#" className="nav-brand">
          <img src={LOGO_IMG} alt="TRU Agent" className="nav-logo" />
          <div className="nav-name">Rebecca Jenkins<span>TRU Agent Real Estate</span></div>
        </a>
        <div className="nav-links">
          <a href="#community">Community</a>
          <a href="#about">About</a>
          <a href="#contact" className="nav-cta">Get in Touch</a>
        </div>
      </nav>

      {/* ── Hero ── */}
      <section className="hero">
        <div className="hero-inner">
          <div className="hero-content">
            <FadeIn><div className="hero-badge">★ Local Market Insider</div></FadeIn>
            <FadeIn delay={0.1}><h1>Your Next Move <em>Starts Here</em></h1></FadeIn>
            <FadeIn delay={0.2}>
              <p className="hero-sub">Whether you're buying your first home, upgrading, or selling — I bring the local expertise and dedicated service to make it happen seamlessly.</p>
            </FadeIn>
            <FadeIn delay={0.3}>
              <div className="hero-btns">
                <a href="#contact" className="btn-primary">Let's Chat About Your Goals →</a>
                <a href="tel:5858802965" className="btn-secondary">📞 Call Now</a>
              </div>
            </FadeIn>
            <FadeIn delay={0.4}>
              <div className="hero-stats">
                <div className="hero-stat"><h3>50+</h3><p>Homes Sold</p></div>
                <div className="hero-stat"><h3>$16M+</h3><p>Sales Volume</p></div>
                <div className="hero-stat"><h3>4+</h3><p>Years Licensed</p></div>
              </div>
            </FadeIn>
          </div>
          <div className="hero-visual">
            <FadeIn delay={0.2}>
              <div className="hero-img-wrapper">
                <img src={AGENT_IMG} alt="Rebecca Jenkins — TRU Agent" />
              </div>
            </FadeIn>
          </div>
        </div>
      </section>

      {/* ── Community Highlights ── */}
      <section className="section-pad community" id="community">
        <FadeIn>
          <div className="gold-line" />
          <h2 className="section-title">Explore Monroe County</h2>
          <p className="section-sub">Discover what makes this region one of New York's best-kept secrets for homeowners.</p>
        </FadeIn>
        <div className="cards-grid">
          {[
            { icon: "🏡", title: "Charming Neighborhoods", desc: "Tree-lined streets, historic villages, and modern subdivisions with incredible character and community." },
            { icon: "🎓", title: "Top-Rated Schools", desc: "Home to some of the highest-rated school districts in New York State, perfect for growing families." },
            { icon: "🌿", title: "Parks & Recreation", desc: "From the Erie Canal trail to Lake Ontario's shores, outdoor enthusiasts will never run out of adventures." },
            { icon: "🍽️", title: "Dining & Culture", desc: "A vibrant food scene, world-class museums, and year-round festivals make every weekend memorable." },
          ].map((c, i) => (
            <FadeIn key={i} delay={i * 0.1}><div className="card"><div className="card-icon">{c.icon}</div><h3>{c.title}</h3><p>{c.desc}</p></div></FadeIn>
          ))}
        </div>
      </section>

      {/* ── Agent Bio ── */}
      <section className="section-pad bio" id="about">
        <div className="bio-inner">
          <FadeIn><img src={BIO_IMG} alt="Rebecca Jenkins" className="bio-photo" /></FadeIn>
          <FadeIn delay={0.15}>
            <div className="bio-text">
              <div className="gold-line" style={{ margin: "0 0 20px" }} />
              <h2>Rebecca Jenkins</h2>
              <p className="bio-role">Buyer Specialist • Listing Agent</p>
              <p className="bio-desc">With over four years of experience and $16 million in sales volume, I specialize in helping buyers and sellers navigate the Monroe County real estate market with confidence. My approach is simple: listen first, strategize second, and deliver results. Whether you're looking for the perfect family home or maximizing your property's value, I'm here to guide you every step of the way.</p>
              <div className="bio-stats-row">
                <div className="bio-stat-card"><h4>50+</h4><p>Homes Sold</p></div>
                <div className="bio-stat-card"><h4>$16M+</h4><p>Volume</p></div>
                <div className="bio-stat-card"><h4>4+</h4><p>Years</p></div>
              </div>
            </div>
          </FadeIn>
        </div>
      </section>

      {/* ── Tabbed Section ── */}
      <section className="section-pad tabs-section">
        <FadeIn>
          <div className="tabs-inner">
            <div className="gold-line" />
            <h2 className="section-title">What We Cover</h2>
            <p className="section-sub">From neighborhood guides to market data, I provide the insights you need to make the best decision.</p>
            <div className="tab-btns">
              {TABS_DATA.map((t, i) => (
                <button key={i} className={`tab-btn ${activeTab === i ? "active" : ""}`} onClick={() => handleTabClick(i)}>{t.label}</button>
              ))}
            </div>
            <div className="tab-content">
              <h3>{tab.title}</h3>
              <p>{tab.desc}</p>
              {tab.items && <ul>{tab.items.map((item, i) => <li key={i}>{item}</li>)}</ul>}
            </div>
            <div className="tab-timer"><div className="tab-timer-fill" style={{ width: `${tabProgress}%` }} /></div>
          </div>
        </FadeIn>
      </section>

      {/* ── Why Monroe County ── */}
      <section className="section-pad why">
        <FadeIn>
          <div className="gold-line" />
          <h2 className="section-title">Why Monroe County?</h2>
          <p className="section-sub">There's a reason families and professionals keep choosing this region as home.</p>
        </FadeIn>
        <div className="why-grid">
          {[
            { icon: "💰", title: "Exceptional Value", desc: "Get more home for your money compared to many Northeast markets, without sacrificing quality of life or amenities." },
            { icon: "🌊", title: "Natural Beauty", desc: "Lake Ontario, the Finger Lakes, the Erie Canal — Monroe County offers breathtaking natural surroundings in every season." },
            { icon: "📈", title: "Growing Economy", desc: "A diversifying economy with major employers in tech, healthcare, education, and manufacturing drives steady growth." },
          ].map((c, i) => (
            <FadeIn key={i} delay={i * 0.12}><div className="why-card"><div className="card-icon">{c.icon}</div><h3>{c.title}</h3><p>{c.desc}</p></div></FadeIn>
          ))}
        </div>
      </section>

      {/* ── Testimonials ── */}
      <section className="section-pad testimonials">
        <FadeIn>
          <div className="gold-line" />
          <h2 className="section-title">What My Clients Say</h2>
          <p className="section-sub">Don't just take my word for it — hear from the families I've helped find their perfect home.</p>
        </FadeIn>
        <div className="testimonials-grid">
          {[
            { text: "Rebecca was outstanding from start to finish. She handled both the sale of our home and the purchase of our new one with incredible diligence, knowledge, and responsiveness. She priced our home perfectly, and it sold the first weekend on the market. On the buying side, she helped us craft a strong offer that was accepted within two hours.", name: "Nick B.", tag: "Bought & Sold in Monroe County" },
            { text: "Rebecca helped us sell our home in Gates in less than a week! She made the whole process very smooth and was available to chat all through the process when questions came up. When it was time to accept the best offer, she negotiated and got us the best possible deal. Rebecca is very capable, intelligent and patient!", name: "Eugenia C.", tag: "Sold in Gates" },
          ].map((t, i) => (
            <FadeIn key={i} delay={i * 0.15}>
              <div className="testimonial-card">
                <div className="testimonial-stars">★★★★★</div>
                <p className="testimonial-text">"{t.text}"</p>
                <div className="testimonial-author">{t.name}<span>{t.tag}</span></div>
              </div>
            </FadeIn>
          ))}
        </div>
        <FadeIn delay={0.3}>
          <div style={{ textAlign: "center" }}>
            <a href="https://www.google.com/search?client=safari&rls=en&q=google+business+rebecca+jenkins+real+estate&ie=UTF-8&oe=UTF-8#mpd=~9312683197513459610/customers/reviews" target="_blank" rel="noopener noreferrer" className="google-reviews-link">See All 12 Reviews on Google →</a>
          </div>
        </FadeIn>
      </section>

      {/* ── Home Valuation ── */}
      <section className="section-pad valuation" id="valuation">
        <FadeIn>
          <div className="val-inner">
            <div className="val-box">
              <div className="val-icon">🏠</div>
              <h2>What's Your Home <em>Really Worth?</em></h2>
              <p>Thinking about selling? Get a free, no-obligation market analysis of your home's current value. I'll pull recent comparable sales and provide a personalized report.</p>
              {valSubmitted ? (
                <div className="val-success">
                  <div style={{ fontSize: "3rem" }}>✓</div>
                  <h3>Request Received!</h3>
                  <p>I'll prepare your home's market analysis and reach out within 24 hours.</p>
                </div>
              ) : (
                <form className="val-form" onSubmit={handleValSubmit}>
                  <input type="text" placeholder="Property address" value={valForm.address} onChange={(e) => setValForm({ ...valForm, address: e.target.value })} />
                  <div className="val-form-row">
                    <input type="text" placeholder="Your name" value={valForm.name} onChange={(e) => setValForm({ ...valForm, name: e.target.value })} />
                    <input type="tel" placeholder="Phone number" value={valForm.phone} onChange={(e) => setValForm({ ...valForm, phone: e.target.value })} />
                  </div>
                  <input type="email" placeholder="Email address" value={valForm.email} onChange={(e) => setValForm({ ...valForm, email: e.target.value })} />
                  {valError && <p style={{ color: "#c55", fontSize: "0.88rem" }}>{valError}</p>}
                  <button type="submit" className="val-submit" disabled={valSubmitting || !isValFormValid}>
                    {valSubmitting ? "Sending..." : "Get My Free Home Valuation →"}
                  </button>
                </form>
              )}
            </div>
          </div>
        </FadeIn>
      </section>

      {/* ── Contact Form ── */}
      <section className="section-pad contact" id="contact">
        <FadeIn>
          <div className="gold-line" />
          <h2 className="section-title">Let's Connect</h2>
          <p className="section-sub">Ready to explore your options? Fill out the form below and I'll reach out to schedule a free, no-obligation conversation.</p>
        </FadeIn>
        <FadeIn delay={0.1}>
          <div className="contact-inner">
            {submitted ? (
              <div className="form-success">
                <div style={{ fontSize: "3rem", marginBottom: 16 }}>✓</div>
                <h3>Thank You!</h3>
                <p>I'll be in touch within 24 hours. Looking forward to connecting!</p>
              </div>
            ) : (
              <form onSubmit={handleSubmit}>
                <div className="form-group">
                  <label>Full Name</label>
                  <input type="text" placeholder="Your full name" value={form.name} onChange={(e) => setForm({ ...form, name: e.target.value })} />
                </div>
                <div className="form-group">
                  <label>Email</label>
                  <input type="email" placeholder="your@email.com" value={form.email} onChange={(e) => setForm({ ...form, email: e.target.value })} />
                </div>
                <div className="form-group">
                  <label>Phone</label>
                  <input type="tel" placeholder="(555) 123-4567" value={form.phone} onChange={(e) => setForm({ ...form, phone: e.target.value })} />
                </div>
                {formError && <p style={{ color: "#c55", fontSize: "0.88rem", marginBottom: 12 }}>{formError}</p>}
                <button type="submit" className="form-submit" disabled={submitting || !isFormValid}>
                  {submitting ? "Sending..." : "Let's Chat About Your Goals"}
                </button>
              </form>
            )}
          </div>
        </FadeIn>
      </section>

      {/* ── Direct Contact ── */}
      <section className="section-pad direct">
        <FadeIn>
          <div className="gold-line" />
          <h2 className="section-title">Prefer a Direct Conversation?</h2>
          <p className="section-sub">I'm always just a call or email away.</p>
        </FadeIn>
        <FadeIn delay={0.1}>
          <div className="direct-grid">
            <div className="direct-card"><div className="card-icon">📞</div><h3>Call or Text</h3><a href="tel:5858802965">(585) 880-2965</a></div>
            <div className="direct-card"><div className="card-icon">✉️</div><h3>Email</h3><a href="mailto:rebeccajenkins@truagent.com">rebeccajenkins@truagent.com</a></div>
          </div>
        </FadeIn>
      </section>

      {/* ── Footer ── */}
      <footer className="footer">
        <div className="footer-brand">
          <img src={LOGO_IMG} alt="TRU Agent" className="footer-logo" />
          <span className="footer-name">Rebecca Jenkins</span>
        </div>
        <p>Licensed Real Estate Salesperson</p>
        <p>Buyer Specialist • Listing Agent • TRU Agent Real Estate</p>
        <p style={{color:'rgba(255,255,255,0.35)',fontSize:'0.78rem',marginTop:'6px'}}>11 Schoen Pl, Pittsford, NY 14534</p>
        <div className="footer-divider" />

        {/* Legal & Compliance Links */}
        <div style={{display:'flex',flexWrap:'wrap',justifyContent:'center',gap:'16px',marginBottom:'16px'}}>
          <a href="https://dos.ny.gov/fair-housing" target="_blank" rel="noopener noreferrer" style={{display:'inline-flex',alignItems:'center',gap:'6px',color:'rgba(255,255,255,0.45)',fontSize:'0.73rem',textDecoration:'none',letterSpacing:'0.5px'}}><span style={{display:'inline-flex',alignItems:'center',justifyContent:'center',width:'24px',height:'24px',border:'1.5px solid rgba(255,255,255,0.35)',borderRadius:'4px',fontSize:'0.65rem',lineHeight:1,fontWeight:700}}>⌂</span>NYS Fair Housing</a>
          <a href="https://truagent.com/wp-content/uploads/2022/04/fairhousingnotice.pdf" target="_blank" rel="noopener noreferrer" style={{color:'rgba(255,255,255,0.45)',fontSize:'0.73rem',textDecoration:'none',letterSpacing:'0.5px'}}>Anti-Discrimination Notice</a>
          <a href="https://truagent.com/wp-content/uploads/2024/08/sop_2024.pdf" target="_blank" rel="noopener noreferrer" style={{color:'rgba(255,255,255,0.45)',fontSize:'0.73rem',textDecoration:'none',letterSpacing:'0.5px'}}>Standardized Operating Procedures</a>
        </div>

        {/* Disclaimers */}
        <div style={{maxWidth:'600px',margin:'0 auto 16px',padding:'0 16px'}}>
          <p style={{color:'rgba(255,255,255,0.28)',fontSize:'0.68rem',lineHeight:1.6,marginBottom:'8px'}}>
            This site is not intended to solicit currently listed properties. All market data is provided for informational purposes only and is not guaranteed. Information deemed reliable but not verified.
          </p>
          <p style={{color:'rgba(255,255,255,0.28)',fontSize:'0.68rem',lineHeight:1.6}}>
            Your privacy matters. Information submitted through this website is used solely to respond to your inquiry and will not be sold or shared with third parties.
          </p>
        </div>

        <p>© {new Date().getFullYear()} Rebecca Jenkins Real Estate. All rights reserved.</p>
      </footer>

      {/* ── Floating Market Update ── */}
      <button className={`market-fab${marketOpen ? " open" : ""}`} onClick={() => setMarketOpen(!marketOpen)} aria-label="Monthly Market Update">
        {marketOpen ? "✕" : "📊"}
      </button>

      <div className={`market-panel${marketOpen ? " open" : ""}`}>
        <div className="market-panel-header">
          <div className="mp-icon">📈</div>
          <div>
            <h3>Monthly Market Update</h3>
            <p>Monroe County — {MARKET_DATA.updated}</p>
          </div>
        </div>

        {MARKET_DATA.stats.map((s, i) => (
          <div className="market-stat" key={i}>
            <div className="market-stat-label">{s.label}</div>
            <div className="market-stat-value">{s.value}</div>
            <div className={`market-stat-change ${s.direction}`}>
              {s.direction === "up" ? "▲" : "▼"} {s.change}
            </div>
          </div>
        ))}

        <p className="market-note">
          Data sourced from Monroe County MLS. Updated monthly.<br />
          Want a personalized market analysis for your area?
        </p>

        <a href="#contact" className="market-cta" onClick={() => setMarketOpen(false)}>
          Get Your Free Market Report →
        </a>
      </div>
    </>
  );
}
